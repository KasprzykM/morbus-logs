<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="6dp"
        android:text="@string/rec_view_date_hist_symp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/date_symptom_log_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView6"
        android:layout_alignParentStart="true"
        android:layout_marginStart="49dp"
        android:layout_marginTop="1dp"
        android:text="TextView"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/edit_symptom_log_icon"
        android:layout_width="44dp"
        android:layout_height="38dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="81dp"
        app:srcCompat="@drawable/ic_edit" />

    <ImageView
        android:id="@+id/delete_symptom_log_icon"
        android:layout_width="40dp"
        android:layout_height="38dp"
        android:layout_alignTop="@+id/edit_symptom_log_icon"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="19dp"
        app:srcCompat="@drawable/ic_cancel" />
</RelativeLayout>